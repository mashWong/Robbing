{
  "manifest_version": 2,
   "content_scripts": [ {
      "exclude_globs": [  ],
      "exclude_matches": [  ],
      "include_globs": [ "*" ],
      "js": [ "jQuery3_3_1.min.js", "script.js"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "converted_from_user_script": true,
   "description": "",
   "key": "iK6h83kI7Qhi7yC3TNQbIdvR8mLPvPu8xszMtDQZ2uE=",
   "name": "mash",
   "version": "1.0",
   "background":
    {
        "scripts": ["index.js"]
    },
   "browser_action": 
    {
        "default_icon": "logo.png",
        "default_title": "干！",
        "default_popup": "pop.html"
    },
	"permissions":
    [
        "contextMenus", // 右键菜单
        "tabs", // 标签
        "notifications", // 通知
        "webRequest", // web请求
        "webRequestBlocking",
        "storage", // 插件本地存储
        "http://*/*", // 可以通过executeScript或者insertCSS访问的网站
        "https://*/*" // 可以通过executeScript或者insertCSS访问的网站
    ],
	"homepage_url": "https://www.baidu.com",
	"devtools_page": "index.html"
}
